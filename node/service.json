{
  "memory": 256,
  "ttl": 10,
  "timeout": 2,
  "minReplicas": 2,
  "maxReplicas": 4,
  "workers": 1,
  "routes": {
    "paymentMethods": {
      "path": "/_v/payment-methods",
      "public": true
    },
    "manifest": {
      "path": "/_v/manifest",
      "public": true
    },
    "authorize": {
      "path": "/_v/payments",
      "public": true
    },
    "settle": {
      "path": "/_v/payments/:paymentId/settlements",
      "public": true
    },
    "refund": {
      "path": "/_v/payments/:paymentId/refunds",
      "public": true
    },
    "cancel": {
      "path": "/_v/payments/:paymentId/cancellations",
      "public": true
    },
    "inbound": {
      "path": "/_v/payments/:paymentId/inbound-request/:action",
      "public": true
    }
  }
}
